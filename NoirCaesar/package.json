  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "cd ./android && ./gradlew app:assembleDebug && ./gradlew installDebug",
    "ios": "react-native run-ios",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}' --fix",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc",
    "build:prod": "gulp set --env=prod",
    "build:staging": "gulp set --env=staging",
    "run-product": "gulp set --env=prod && react-native start",
    "ios-dev": "ENVFILE=.env.development react-native run-ios",
    "android-dev": "ENVFILE=.env.development react-native run-android",
    "post-install": "npm run jetify",
    "jetify": "npx jetify",
    "modify-alphabet-list-view": "cp -rf ./src/components/Scripts/SelectableSectionsListView.js ./node_modules/react-native-alphabetlistview/components",
    "modify-bottombar": "cp ./src/components/Scripts/BottomTabs.java ./node_modules/react-native-navigation/lib/android/app/src/main/java/com/reactnativenavigation/views/BottomTabs.java",
    "modify-react-native-material-tabs": "cp ./src/components/Scripts/MaterialTabs.tsx ./node_modules/react-native-material-tabs/src/components/MaterialTabs.tsx",
    "modify-react-native-collapsible-tabs": "cp ./src/components/Scripts/CollapsibleTabs.js ./node_modules/react-native-collapsible-tabs/CollapsibleTabs.js",
    "modify-av-player-wrapper": "cp ./src/components/Scripts/AVPlayerWrapper.swift ./node_modules/react-native-track-player/ios/RNTrackPlayer/Vendor/AudioPlayer/AVPlayerWrapper/AVPlayerWrapper.swift",
    "modify-audio-player": "cp ./src/components/Scripts/AudioPlayer.swift ./node_modules/react-native-track-player/ios/RNTrackPlayer/Vendor/AudioPlayer/AudioPlayer.swift",
    "modify-android-pdf-viewer-release": "mkdir -p ./node_modules/react-native-pdf/android/libs/ && cp -rf ./src/assets/dependencies/android-pdf-viewer-release.aar ./node_modules/react-native-pdf/android/libs",
    "modify-android-pdf-viewer-gradle": "cp ./src/assets/dependencies/android-pdf-viewer-build.gradle ./node_modules/react-native-pdf/android/build.gradle",
    "postinstall": "npm run modify-alphabet-list-view & npm run modify-bottombar & npm run jetify & npm run modify-react-native-material-tabs & npm run modify-react-native-collapsible-tabs & npm run modify-audio-player & npm run modify-av-player-wrapper & npm run modify-android-pdf-viewer-release & npm run modify-android-pdf-viewer-gradle"
  },
