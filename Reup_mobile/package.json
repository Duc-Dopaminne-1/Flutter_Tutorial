{
  "name": "rnTemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "cd ./android && ./gradlew app:assembleDebug && ./gradlew installDebug",
    "ios": "react-native run-ios",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}' --fix",
    "test": "npm run type-check && npm run lint",
    "setup": "rm -rf Package-lock.json && npm install && cd ios && rm -rf Pods && rm -rf Podfile.lock && pod install && cd .. && npx jetify",
    "type-check": "tsc",
    "run-ios-manager-dev": "ENVFILE=.env.dev react-native run-ios --scheme \"ReupMobile-Manager-Dev\"  --configuration Manager_Dev_Debug",
    "run-ios-manager-prod": "ENVFILE=.env.prod react-native run-ios --scheme \"ReupMobile-Manager-Prod\"  --configuration Manager_Prod_Debug",
    "run-ios-tenant-dev": "ENVFILE=.env.tenant.dev react-native run-ios --scheme \"ReupMobile-Tenant-Dev\"  --configuration Tenant_Dev_Debug",
    "run-ios-tenant-prod": "ENVFILE=.env.tenant.prod react-native run-ios --scheme \"ReupMobile-Tenant-Prod\"  --configuration Tenant_Prod_Debug",
    "build-ios-manager-dev": "cd ios && xcodebuild archive -workspace rnReupMobile.xcworkspace -scheme ReupMobile-Manager-Dev -archivePath ~/desktop/ReupMobile-Manager-Dev.xcarchive",
    "build-ios-manager-prod": "cd ios && xcodebuild archive -workspace rnReupMobile.xcworkspace -scheme ReupMobile-Manager-Prod -archivePath ~/desktop/ReupMobile-Manager-Prod.xcarchive",
    "build-ios-tenant-dev": "cd ios && xcodebuild archive -workspace rnReupMobile.xcworkspace -scheme ReupMobile-Tenant-Dev -archivePath ~/desktop/ReupMobile-Tenant-Dev.xcarchive",
    "build-ios-tenant-prod": "cd ios && xcodebuild archive -workspace rnReupMobile.xcworkspace -scheme ReupMobile-Tenant-Prod -archivePath ~/desktop/ReupMobile-Tenant-Prod.xcarchive",
    "run-android-manager-dev": "ENVFILE=.env.dev react-native run-android --variant=reupManagerDevDebug",
    "run-android-manager-prod": "ENVFILE=.env.prod react-native run-android --variant=reupManageProdDebug",
    "run-android-tenant-dev": "ENVFILE=.env.tenant.dev react-native run-android --variant=reupTenantDevDebug",
    "run-android-tenant-prod": "ENVFILE=.env.tenant.prod react-native run-android --variant=reupTenantProdDebug",
    "build-android-manager-dev": "cd android && ENVFILE=.env.dev ./gradlew assembleReupManageDevRelease",
    "build-android-manager-prod": "cd android && ENVFILE=.env.prod ./gradlew assembleReupManageProdRelease",
    "build-android-tenant-dev": "cd android && ENVFILE=.env.tenant.dev ./gradlew assembleReupTenantDevRelease",
    "build-android-tenant-prod": "cd android && ENVFILE=.env.tenant.prod ./gradlew assembleReupTenantProdRelease",
    "build:prod": "gulp set --env=prod",
    "build:staging": "gulp set --env=staging",
    "run-product": "gulp set --env=prod && react-native start",
    "jetify": "npx jetify",
    "ios-dev": "ENVFILE=.env.dev react-native run-ios",
    "post-install": "npm run jetify",
    "modify-react-native-material-tabs": "cp ./src/components/Scripts/MaterialTabs.tsx ./node_modules/react-native-material-tabs/src/components/MaterialTabs.tsx",
    "modify-react-native-collapsible-tabs": "cp ./src/components/Scripts/CollapsibleTabs.js ./node_modules/react-native-collapsible-tabs/CollapsibleTabs.js",
    "modify-agenda": "cp ./src/components/Scripts/CustomAgendaList/index.js ./node_modules/react-native-calendars/src/agenda/index.js",
    "modify-agenda-style": "cp ./src/components/Scripts/CustomAgendaList/style.js ./node_modules/react-native-calendars/src/agenda/reservation-list/style.js",
    "modify-agenda-reservation-list": "cp ./src/components/Scripts/CustomAgendaList/ReservationList/index.js ./node_modules/react-native-calendars/src/agenda/reservation-list/index.js",
    "modify-agenda-reservation": "cp ./src/components/Scripts/CustomAgendaList/reservation.js ./node_modules/react-native-calendars/src/agenda/reservation-list/reservation.js",
    "modify-react-native-chart-kit": "cp ./src/components/Scripts/LineChart.js ./node_modules/react-native-chart-kit/dist/line-chart/LineChart.js",
    "modify-react-native-status-bar": "cp ./src/components/Scripts/RCTStatusBarManager.m ./node_modules/react-native/React/Modules/RCTStatusBarManager.m",
    "postinstall": "npm run jetify & npm run modify-react-native-material-tabs & npm run modify-react-native-collapsible-tabs & npm run modify-agenda & npm run modify-agenda-style & npm run modify-agenda-reservation & npm run modify-agenda-reservation-list & npm run modify-agenda-style & npm run modify-react-native-status-bar"
  },
