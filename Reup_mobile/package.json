    "react-test-renderer": "16.8.6",
    "reactotron-react-native": "^4.0.2",
    "typescript": "3.5.3"
  },
  "jest": {
    "preset": "react-native"
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/ReupMobile.app",
        "build": "xcodebuild -workspace ios/rnTemplate.xcworkspace -scheme ReupMobile -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 11, iOS 13.3"
      }
    },
    "test-runner": "mocha"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && tsc --noEmit"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run format",
      "npm run lint",
      "git add"
    ]
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "react-native": {
    "fs": "react-native-level-fs"
  },
  "browser": {
    "fs": "react-native-level-fs"
  }
}
