{
  "name": "BidBid",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lintDefault": "eslint .",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}' --fix",
    "setup-dev": "rm -rf Package-lock.json && npm install && cd ios && rm -rf Pods && rm -rf Podfile.lock && pod install && cd .. && npx jetify",
    "run-android-dev": "ENVFILE=.env.dev react-native run-android --variant=devDebug",
    "run-android-staging": "ENVFILE=.env.staging react-native run-android --variant=stagingDebug",
    "run-android-prod": "ENVFILE=.env react-native run-android --variant=productionDebug",
    "build-android-dev": "npx jetify && cd android && ENVFILE=.env.dev ./gradlew assembleDevRelease",
    "build-android-staging": "npx jetify && cd android && ENVFILE=.env.staging ./gradlew assembleStagingRelease",
    "build-android-prod": "npx jetify && cd android && ENVFILE=.env ./gradlew assembleProductionRelease",
    "build-android-prod-store": "npx jetify && cd android && ENVFILE=.env ./gradlew bundleProductionRelease",
    "run-ios-dev": "ENVFILE=.env.dev react-native run-ios --scheme \"BidBid-Dev\" --configuration \"Dev Debug\"",
    "run-ios-prod": "ENVFILE=.env react-native run-ios --scheme \"BidBid\" --configuration \"Debug\"",
    "run-ios-staging": "ENVFILE=.env.staging react-native run-ios --scheme \"BidBid-Staging\" --configuration \"Staging Debug\"",
    "clean-build": "cd android && ./gradlew clean && cd ..",
    "rebuild": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && yarn clear-gradlew"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": []
  },
