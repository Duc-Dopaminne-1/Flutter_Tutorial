{
  "name": "ShowSourcing",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "bundle-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "bundle-ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/ShowSourcing/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "clean": "npm start -- --reset-cache",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "precommit": "pretty-quick --staged",
    "prettier": "prettier --write 'app/**/*.{js,jsx,ts,tsx}'",
    "postinstall": "react-native-schemes-manager",
    "open-dir": "open app/build/outputs/apk/release",
    "build-dev2": "cd android && ENVFILE=.env.dev2 ./gradlew assembleDev2Release",
    "build-dev3": "cd android && ENVFILE=.env.dev3 ./gradlew assembleDev3Release",
    "build-sta": "cd android && ENVFILE=.env.staging ./gradlew assembleStagingRelease",
    "build-prod": "cd android && ENVFILE=.env.production ./gradlew assembleProductionRelease",
    "build-aap-dev2": "cd android && ENVFILE=.env.dev2 ./gradlew bundleDev2Release",
    "build-aap-dev3": "cd android && ENVFILE=.env.dev3 ./gradlew bundleDev3Release",
    "build-aap-sta": "cd android && ENVFILE=.env.staging ./gradlew bundleStagingRelease",
    "build-aap-prod": "cd android && ENVFILE=.env.production ./gradlew bundleProductionRelease",
    "archive-dev2": "cd ios && xcodebuild -workspace ShowSourcing.xcworkspace -scheme \"ShowSourcing Dev 2\" -sdk iphoneos -configuration AppStoreDistribution archive",
    "archive-prod": "cd ios && xcodebuild -workspace ShowSourcing.xcworkspace -scheme \"ShowSourcing Production\" -sdk iphoneos -configuration AppStoreDistribution archive",
    "run-dev2": "ENVFILE=.env.dev2 && react-native run-android --variant=dev2Debug",
    "run-dev3": "ENVFILE=.env.dev3 && react-native run-android --variant=dev3Debug",
    "run-sta": "ENVFILE=.env.staging && react-native run-android --variant=stagingDebug",
    "run-prod": "ENVFILE=.env.production && react-native run-android --variant=productionDebug",
    "run-dev2-win": "SET ENVFILE=.env.dev2 && react-native run-android --variant=dev2Debug",
    "run-dev3-win": "SET ENVFILE=.env.dev3 && react-native run-android --variant=dev3Debug",
    "run-sta-win": "SET ENVFILE=.env.staging && react-native run-android --variant=stagingDebug",
    "run-prod-win": "SET ENVFILE=.env.staging && react-native run-android --variant=productionDebug",
    "run-dev2-ios": "react-native run-ios --configuration Dev2.Debug",
    "run-dev3-ios": "react-native run-ios --configuration Dev3.Debug",
    "run-sta-ios": "react-native run-ios --configuration Staging.Debug",
    "run-prod-ios": "react-native run-ios --configuration Debug",
    "debug-android": "adb reverse tcp:8081 tcp:8081 && adb reverse tcp:9090 tcp:9090",
    "fix-3": "cd node_modules/react-native/scripts && ./ios-install-third-party.sh && cd ../../../ && cd node_modules/react-native/third-party/glog-0.3.5/ && ../../scripts/ios-configure-glog.sh && cd ../../../../",
    "menu": "adb shell input keyevent KEYCODE_MENU",
    "migrate-android-x": "cd migrateAndroidX && ./script.sh",
    "open-android-build": "open android/app/build/outputs/apk"
  },
  "xcodeSchemes": {
    "Debug": [
      "Dev2.Debug",
      "Dev3.Debug",
      "Staging.Debug",
      "Debug"
    ],
    "Release": [
      "Dev2.Release",
      "Dev3.Release",
      "Staging.Release",
      "Release"
    ],
    "projectDirectory": "iOS"
  },
  "projectDirectory": "iOS",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@dudigital/react-native-zoomable-view": "^1.0.12",
