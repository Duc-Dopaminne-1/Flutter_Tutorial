{
  "name": "sociallens",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "reactotron": "adb reverse tcp:9090 tcp:9090",
    "setup-all": "rm -rf Package-lock.json && npm install && cd ios && rm -rf Pods && rm -rf Podfile.lock && pod install && cd .. && npx jetify",
    "run-prod": "ENVFILE=.env react-native run-android --variant=productionDebug",
    "run-staging": "ENVFILE=.env.stage react-native run-android --variant=stagingDebug",
    "run-dev": "ENVFILE=.env.dev react-native run-android --variant=devDebug",
    "build-prod": "cd android && ENVFILE=.env ./gradlew assembleProductionRelease",
    "build-staging": "cd android && ENVFILE=.env.stage ./gradlew assembleStagingRelease",
    "build-dev": "cd android && ENVFILE=.env.dev ./gradlew assembleDevRelease"
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "^1.0.0",
    "@react-native-community/art": "^1.2.0",
    "@react-native-community/async-storage": "^1.9.0",
    "@react-native-community/datetimepicker": "^2.4.2",
    "@react-native-community/google-signin": "^4.0.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/toolbar-android": "^0.1.0-rc.2",
    "axios": "^0.19.2",
    "lodash": "^4.17.19",
    "moment": "^2.25.1",
    "prop-types": "^15.7.2",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-actionsheet": "^2.4.2",
    "react-native-config": "^1.2.0",
    "react-native-dash": "0.0.11",
    "react-native-datepicker": "^1.7.2",
    "react-native-fast-image": "^8.1.5",
    "react-native-fbsdk": "^1.1.2",
    "react-native-firebase": "^5.6.0",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-get-random-values": "^1.4.0",
    "react-native-gifted-chat": "git+https://github.com/StagasaurusRex/react-native-gifted-chat.git#upgrade-action-sheet",
    "react-native-i18n": "^2.0.15",
    "react-native-image-crop-picker": "^0.31.1",
    "react-native-image-picker": "^2.3.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-modal": "^11.5.6",
    "react-native-onesignal": "^3.7.3",
    "react-native-picker-select": "^7.0.0",
    "react-native-progress": "^4.1.2",
    "react-native-reanimated": "^1.8.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.7.0",
    "react-native-vector-icons": "6.3.0",
    "react-native-video": "^4.4.5",
    "react-native-youtube": "^2.0.1",
    "react-navigation": "^4.3.9",
    "react-navigation-stack": "^2.4.0",
    "react-navigation-tabs": "^2.8.13",
    "reactotron-react-native": "^5.0.0",
    "rxjs": "^6.6.2",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/runtime": "7.9.6",
    "@react-native-community/eslint-config": "0.0.5",
    "babel-jest": "24.9.0",
    "eslint": "6.8.0",
    "jest": "24.9.0",
    "metro-react-native-babel-preset": "0.58.0",
    "react-test-renderer": "16.11.0"
  },
  "jest": {
    "preset": "react-native"
  }
